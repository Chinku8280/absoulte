                  <label class="form-check-label" for="default-address-radio-${index}">
                    Default Address
                  </label>
                </div>
                <hr class="my-3">
              </div>
            </label>
          </div>
        </div>
      `);
                    $(`#radio-card-billing-${index}`).on('click', function() {
